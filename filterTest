import cv

img = cv.LoadImage("SomePicture.jpg")
  
isoRed(img, True)
cv.saveImage("isoRed - True")
    
isoRed(img, False)
cv.saveImage("isoRed - False")

isoRed2(img, True)
cv.saveImage("isoRed2 - True")

isoRed2(img, False)
cv.saveImage("isoRed2 - False")

isoBlue(img, True)
cv.saveImage("isoBlue - True")

isoBlue(img, False)
cv.saveImage("isoBlue - False")
    
